<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>servletcaracliga</servlet-name>
        <servlet-class>control.servletcaracliga</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletcaracequipo</servlet-name>
        <servlet-class>control.servletcaracequipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletcaracjugador</servlet-name>
        <servlet-class>control.servletcaracjugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletclasliga</servlet-name>
        <servlet-class>control.servletclasliga</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletplantilla</servlet-name>
        <servlet-class>control.servletplantilla</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletnacionalidad</servlet-name>
        <servlet-class>control.servletnacionalidad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletcaracliga</servlet-name>
        <url-pattern>/servletcaracliga</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletcaracequipo</servlet-name>
        <url-pattern>/servletcaracequipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletcaracjugador</servlet-name>
        <url-pattern>/servletcaracjugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletclasliga</servlet-name>
        <url-pattern>/servletclasliga</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletplantilla</servlet-name>
        <url-pattern>/servletplantilla</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletnacionalidad</servlet-name>
        <url-pattern>/servletnacionalidad</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
